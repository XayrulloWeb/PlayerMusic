{
  "expo": {
    "name": "my-clean-app",
    "slug": "my-clean-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#030318"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppleMusicUsageDescription": "This app uses your music library to play your songs.",
        "kTCCServiceMediaLibrary": "This app uses your music library to play your songs."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.xayrullodev.mycleanapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-audio"
    ],
    "extra": {
      "eas": {
        "projectId": "18fb7ba2-bf14-452e-96be-eafeed04c6c8"
      }
    }
  }
}
